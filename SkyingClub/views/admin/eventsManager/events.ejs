            <div class="card-body">
                <div id="tableProducts" class="dataTables_wrapper dt-bootstrap4"><div class="row"><div class="col-sm-12 col-md-6"><div class="dt-buttons btn-group flex-wrap">               
                    <button class="btn btn-secondary buttons-copy buttons-html5" tabindex="0" aria-controls="example1" type="button">
                      <span>Copy</span>
                    </button> 
                    <button class="btn btn-secondary buttons-csv buttons-html5" tabindex="0" aria-controls="example1" type="button">
                        <span>CSV</span>
                    </button> 
                    <button class="btn btn-secondary buttons-excel buttons-html5" tabindex="0" aria-controls="example1" type="button">
                        <span>Excel</span>
                    </button> 
                    <button class="btn btn-secondary buttons-pdf buttons-html5" tabindex="0" aria-controls="example1" type="button">
                        <span>PDF</span>
                    </button> 
                    <button class="btn btn-secondary buttons-print" tabindex="0" aria-controls="example1" type="button">
                        <span>Print</span>
                    </button> 
                <div class="btn-group">
                    <button class="btn btn-secondary buttons-collection dropdown-toggle buttons-colvis" tabindex="0" aria-controls="example1" type="button" aria-haspopup="true" aria-expanded="false">
                        <span>Column visibility</span>
                    </button></div> 
                </div></div>

            </div>
        </div>
        <div class="row"><div class="col-sm-12">
            <table id="tableData" class="table table-bordered table-striped dataTable dtr-inline" aria-describedby="example1_info">
                <thead>
                <tr>
                  <th data-column="sttPT" data-order= "dest" class="text-center">#</th>
                    <th data-column="idPT" data-order= "dest" class="text-center">ID</th>
                    <th data-column="namePT" data-order= "dest" class="text-center">Tên sự kiện</th>
                    <th data-column="trademarkPT" data-order= "dest" class="text-center">Môn thể thao</th>
                    <th data-column="orderPT" data-order= "dest" class="text-center">Thời gian bắt đầu</th>
                    <th data-column="orderPT" data-order= "dest" class="text-center">Thời gian kết thúc</th>
                    <th data-column="sexPT" data-order= "dest" class="text-center">Địa điểm</th> 
                    <th style="width: 17%"></th></tr>
                </thead>
                <tbody>
                  <% eventMo.forEach(function(prod){ %>
                    <tr class="odd">
                      <td class="text-center"></td>
                      <td> <a class="dtr-control sorting_1" tabindex="0">#<%- prod._id %></a></td>
                      <td class="text-center"><%- prod.name %></td>
                      <td class="text-center"><%- prod.sport  %></td>
                      <td class="text-center"><%- prod.timeStart  %></td>
                      <td class="text-center"><%- prod.timeEnd %></td>
                      <td class="text-center"><%- prod.address %></td>
                      <td class="text-center"t>
                        <a class="btn btn-primary btn-sm" href="#">
                            <i class="fas fa-folder">
                            </i>
                            View
                        </a>
                        <a class="btn btn-info btn-sm" href="#">
                            <i class="fas fa-pencil-alt">
                            </i>
                            Edit
                        </a>
                        <a class="btn btn-danger btn-sm" href="#">
                            <i class="fas fa-trash">
                            </i>
                            Delete
                        </a>
                    </td>
                    </tr>
                  <%  }); %>
            </tbody>
              </table>
            </div>
            </div> 
            <div class="row">
                <div class="col-sm-12 col-md-5">
                    <div class="dataTables_info" id="example1_info" role="status" aria-live="polite">Showing 1 to 10 of 57 entries</div>
                </div>
